	<queryString>
		<![CDATA[SELECT
    a.section_id AS sectionId,
    s.status AS sectionStatus,
    s.name AS sectionName,
    a.id AS accountId,
    a.code AS accountCode,
    a.name AS accountName,
    a.status AS accountStatus
FROM
    account AS a,
    chart_section AS s
WHERE
    a.section_id = s.id AND
    a.security_code = $P{companySecurityCode}
    $P!{sectionClause} $P!{accountClause}
ORDER BY
    s.tree_sequence,
    a.code]]>
